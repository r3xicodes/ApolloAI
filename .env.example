# Copy this file to .env.local (DO NOT commit .env.local)
# Fill values with your environment-specific secrets

# Upstash Redis (optional - required for persistence)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# JWT secret used to sign tokens (generate using crypto.randomBytes)
JWT_SECRET=

# Optional LLM settings (e.g. OpenAI)
AI_API_URL=https://api.openai.com/v1/chat/completions
AI_API_KEY=
